{
    "root": true,
    "extends": [
        "eslint:recommended",
        "next",
        "prettier",
        "plugin:tailwindcss/recommended"
    ],
    "rules": {
        "no-unused-vars": "warn"
    },
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js"],
            "parser": "@typescript-eslint/parser"
        }
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}
